<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
	PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
	"http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>

	<!-- Default page to serve -->
	<welcome-file-list>
		<welcome-file>View003.html</welcome-file>
	</welcome-file-list>
	
	<servlet>
		<servlet-name>userServiceServlet</servlet-name>
		<servlet-class>com.acn.webapp.eloa.server.UserServiceImpl</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>userServiceServlet</servlet-name>
		<url-pattern>/eloaApp001/userService</url-pattern>
	</servlet-mapping>
</web-app>
